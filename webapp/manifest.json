{
  "_version": "1.60.0",
  "sap.app": {
    "id": "trackingapp",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.4",
      "toolsId": "45bf6033-ee34-41af-81b8-0d994e9dd1b8"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata4/serpcc/api_trackship_v4/srvd_a2x/serpcc/api_trackship/0001/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "tracking-display": {
          "semanticObject": "tracking",
          "action": "display",
          "title": "{{tracking-display.flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        },
        "tracking_app-display": {
          "semanticObject": "tracking_app",
          "action": "display",
          "title": "{{tracking_app-display.flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.120.23",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "trackingapp.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "controlAggregation": "beginColumnPages",
        "controlId": "layout",
        "type": "View",
        "viewType": "XML",
        "path": "trackingapp.view"
      },
      "routes": [
        {
          "layout": "OneColumn",
          "name": "tracking",
          "pattern": "",
          "target": [
            "tracking"
          ]
        },
        {
          "layout": "TwoColumnsMidExpanded",
          "name": "details",
          "pattern": "{key}",
          "target": [
            "tracking",
            "details"
          ]
        },
        {
          "layout": "MidColumnFullScreen",
          "name": "detailsNoList",
          "pattern": "{key}?noList",
          "target": [
            "empty",
            "details"
          ]
        }
      ],
      "targets": {
        "empty": {
          "id": "empty",
          "level": 1,
          "name": "Empty"
        },
        "tracking": {
          "id": "tracking",
          "level": 1,
          "name": "Tracking"
        },
        "details": {
          "controlAggregation": "midColumnPages",
          "id": "details",
          "level": 1,
          "name": "Details"
        }
      }
    },
    "rootView": {
      "viewName": "trackingapp.view.App",
      "type": "XML",
      "id": "App"
    }
  }
}
